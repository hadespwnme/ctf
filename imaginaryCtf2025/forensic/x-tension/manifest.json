{
  "manifest_version": 3,
  "name": "Funny Cat Pics Generator",
  "version": "1.0",
  "description": "Sends cat pics or something",
  "permissions": [
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
